<select ng-model="styleFilter" ng-init="styleFilter = styleFilter || styles[0]" ng-options="style for style in styles">
</select>
<table class="table table-hover">
	<tr><th>Style</th><th>Name</th><th>Date(s)</th><th>Next Dance</th></tr>
	<tr ng-repeat="dance in dances | filter:{style: styleFilter}" dance="dance">
	</tr>
</table>